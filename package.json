{
    "name": "homebridge-schedule",
    "version": "1.0.0",
    "description": "Scheduled, repeatable triggers for Homebridge automation with Homekit",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kbrashears5/homebridge-schedule.git"
    },
    "bugs": {
        "url": "https://github.com/kbrashears5/homebridge-schedule/issues"
    },
    "homepage": "https://github.com/kbrashears5/homebridge-schedule#readme",
    "author": "Kevin Brashears",
    "license": "MIT",
    "keywords": [
        "homebridge-plugin",
        "homebridge",
        "typescript",
        "plugin",
        "schedule",
        "repeat",
        "automation"
    ],
    "main": "dist/schedule-accessory.js",
    "dependencies": {
        "cron": "~1.8.2",
        "typescript-helper-functions": "~1.0.99"
    },
    "devDependencies": {
        "@types/cron": "~1.7.2",
        "@types/node": "~14.14.19",
        "homebridge": "~1.1.7",
        "npm-run-all": "~4.1.5",
        "rimraf": "~3.0.2",
        "tslint": "~6.1.3",
        "typescript": "~4.1.3"
    },
    "files": [
        "LICENSE",
        "dist",
        "config.schema.json"
    ],
    "scripts": {
        "build": "rimraf ./dist && tsc",
        "clean": "rimraf ./dist",
        "prepublishOnly": "npm run build",
        "postpublish": "npm run clean",
        "test": "echo \"Error: no test specified\" && exit 1",
        "update": "run-s update:*",
        "update:cron": "npm i cron@latest",
        "update:typescript-helper-functions": "npm i typescript-helper-functions@latest",
        "update:types-cron": "npm i @types/cron@latest --save-dev",
        "update:types-node": "npm i @types/node@latest --save-dev",
        "update:homebridge": "npm i homebridge@latest --save-dev",
        "update:npm-run-all": "npm i npm-run-all@latest --save-dev",
        "update:rimraf": "npm i rimraf@latest --save-dev",
        "update:tslint": "npm i tslint@latest --save-dev",
        "update:typescript": "npm i typescript@latest --save-dev"
    },
    "engines": {
        "homebridge": ">=1.0.0"
    }
}
